<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="soom.control.JanelaCadastroServico"
            prefHeight="200.0" prefWidth="300.0">


    <content>
        <GridPane>
            <Label GridPane.rowIndex="0" GridPane.columnIndex="0" text="Categoria:" labelFor="$tfCategoria"/>
            <TextField GridPane.rowIndex="0" GridPane.columnIndex="1" fx:id="tfCategoria"/>

            <Label GridPane.rowIndex="1" GridPane.columnIndex="0" text="Nome:" labelFor="$tfNome"/>
            <TextField GridPane.rowIndex="1" GridPane.columnIndex="1" fx:id="tfNome"/>

            <Label GridPane.rowIndex="2" GridPane.columnIndex="0" text="Valor (R$):" labelFor="$tfValor"/>
            <TextField GridPane.rowIndex="2" GridPane.columnIndex="1" fx:id="tfValor"/>
        </GridPane>
    </content>

</DialogPane>
