<?xml version="1.0" encoding="UTF-8"?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>


<BorderPane xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        fx:controller="view.JanelaDeletarCliente"
        prefHeight="400.0" prefWidth="600.0">


        <top >
            <HBox alignment="CENTER" >
                <Text fx:id="txtLogo" text="Remover Serviço" />
            </HBox>
        </top>
        <center>
            <GridPane alignment="CENTER">
                <VBox alignment="CENTER" spacing="10">
                    <ComboBox promptText="Serviço" GridPane.rowIndex="0" GridPane.columnIndex="0" fx:id="cbServico" ></ComboBox>
                    <HBox alignment="CENTER" spacing="10">
                        <Button GridPane.rowIndex="1" GridPane.columnIndex="0" onAction="#Confirmar" text="Remover!"></Button>
                        <Button onAction="#Voltar" text="Voltar"></Button>

                    </HBox>
                </VBox>
            </GridPane>
        </center>
    </BorderPane>

