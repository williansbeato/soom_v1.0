<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<!--        onAction="#btLogar"-->

<BorderPane fx:id="cena" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="soom.control.JanelaLogin">
<!--    -->
    <center>
        <GridPane alignment="CENTER">
            <Label fx:id="lbMsg" GridPane.columnIndex="0" GridPane.rowIndex="0" />
            <Label text="Login:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
            <TextField fx:id="tfLogin" GridPane.columnIndex="0" GridPane.rowIndex="2" />
            <Label text="Senha:" GridPane.columnIndex="0" GridPane.rowIndex="3" />
            <PasswordField fx:id="tfSenha" GridPane.columnIndex="0" GridPane.rowIndex="4" />
            <Button fx:id="submitButton" text="Logar" onAction="#Login" GridPane.columnIndex="0" GridPane.rowIndex="5" />
         <columnConstraints>
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
         </rowConstraints>
        </GridPane>
    </center>
</BorderPane>
